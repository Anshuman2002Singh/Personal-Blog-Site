<%- include("partials/header"); -%>                   
<h1> COMPOSE</h1>
<form action="/compose" method="post">
    <div class="form-group">
    <label>Title</label>
    <input class="form-control" type="text" name="postTitle" placeholder="Enter your text">
    <label>Post</label>
    <textarea class="form-control" name="postBody" rows="5" cols="80"></textarea>
</div>
<button class="btn btn-primary" type="submit" name="button">Publish</button>
</form>
